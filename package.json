{
  "name": "react-weather-test",
  "private": true,
  "version": "0.0.0",
  "description": "React Weather test app",
  "license": "MIT",
  "repository": "VovanR/react-weather-test",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:copy && webpack",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:copy": "cp ./src/index.html ./dist/",
    "deploy": "gh-pages-deploy",
    "prepush": "npm test",
    "release-major": "mversion major",
    "release-minor": "mversion minor",
    "release-patch": "mversion patch",
    "start": "webpack-dev-server",
    "test": "xo"
  },
  "keywords": [
    "react",
    "application",
    "app",
    "weather"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "conventional-changelog-cli": "^1.2.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.11.1",
    "eslint-config-xo": "^0.14.1",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "husky": "^0.11.4",
    "mversion": "^1.10.1",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "xo": "^0.15.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "xo": {
    "extends": "xo-react"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "dist"
    ],
    "noprompt": false
  }
}
